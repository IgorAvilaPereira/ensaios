-- Name: autoria_pkey; Type: CONSTRAINT; Schema: public; Owner: postgres; Tablespace: 
--

ALTER TABLE ONLY autoria
    ADD CONSTRAINT autoria_pkey PRIMARY KEY (id_membro, id_publicacao);


