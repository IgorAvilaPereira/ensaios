-- Name: capitulo_pkey; Type: CONSTRAINT; Schema: public; Owner: postgres; Tablespace: 
--

ALTER TABLE ONLY capitulo
    ADD CONSTRAINT capitulo_pkey PRIMARY KEY (id_capitulo);


