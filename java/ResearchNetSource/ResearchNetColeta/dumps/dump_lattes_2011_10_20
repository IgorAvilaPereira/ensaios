-- Name: conferencia_pkey; Type: CONSTRAINT; Schema: public; Owner: postgres; Tablespace: 
--

ALTER TABLE ONLY conferencia
    ADD CONSTRAINT conferencia_pkey PRIMARY KEY (id_conferencia);


