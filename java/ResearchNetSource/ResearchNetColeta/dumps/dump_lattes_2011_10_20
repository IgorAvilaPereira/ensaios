-- Name: adjacencia_pkey; Type: CONSTRAINT; Schema: public; Owner: postgres; Tablespace: 
--

ALTER TABLE ONLY adjacencia
    ADD CONSTRAINT adjacencia_pkey PRIMARY KEY (membro1, membro2);


